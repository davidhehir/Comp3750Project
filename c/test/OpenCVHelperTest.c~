#include <stdio.h>
#include "../src/OpenCVHelper.h"
#include "minunit.h"

int tests_run = 0;

int x = 6;

static char * test_framework()
{
  mu_assert("error, x!=6",x==6);
  return 0;
}

static char * OpenCVHelperTestSuite()
{
  mu_run_test(test_framework);
  return 0;
}

int main(int argc, char **argv)
{
  char *result = OpenCVTestSuite();
  if (result != 0)
    {
      printf("%s\n",result);
    }
  else
    {
      printf("Tests Rune: %d\n",tests_run);
    }

  return result != 0;
}
